  <!-- CVE-2018-1115 is related to the Postgresql SERVER, not the driver library -->
   <suppress>
      <notes><![CDATA[
      file name: postgresql-42.2.2.jar
      ]]></notes>
      <gav regex="true">^org\.postgresql:postgresql:.*$</gav>
      <cve>CVE-2018-1115</cve>
   </suppress>

